{
  "type" : [ "tonic-query-data-model", "slice-prober" ],
  "arguments": {
      "time": {
         "ui": "slider",
         "values": [ "0", "1", "2" ],
         "label": "Time" },
      "field": {
        "values": [ "temperature" ]
      },
      "slice": {
        "values": [ "0", "1", "2" ]
      }
  },
  "arguments_order": [ "field", "time" ],
  "data": [
      {
        "name": "slice",
        "type": "blob",
        "mimeType": "image/png",
        "pattern": "{time}/{field}_{slice}.png"
      }
  ],
  "metadata": {
      "title": "Ocean simulation data",
      "type": "probe-slice",
      "id": "mpas-probe-data",
      "description": "Some simulation data from MPAS"
  },
  "slice-prober": {
    "sprite_size" : 10,
    "dimensions": [ 500, 250, 30 ],
    "spacing": [ 1.0, 1.0, 4.0 ],
    "ranges" : {
      "temperature": [-5, 30]
    }
  }
}
